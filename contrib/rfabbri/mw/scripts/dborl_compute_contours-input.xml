Contour_Tracer-len_threshold="20"
Contour_Tracer-nsteps="1"
Contour_Tracer-sigma="1">
</Contour_Tracer>
<Edge_Detection_Color Edge_Detection_Color-bLoadComps="off"
Edge_Detection_Color-breduce="off"
Edge_Detection_Color-col_conv="2"
Edge_Detection_Color-conv_algo="0"
Edge_Detection_Color-grad_op="0"
Edge_Detection_Color-int_factor="1"
Edge_Detection_Color-parabola_fit="0"
Edge_Detection_Color-sigma="1"
Edge_Detection_Color-thresh="2">
