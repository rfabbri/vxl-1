[auto-props]
# Binary files
*.bvl                           = svn:mime-type=application/octet-stream
*.esf                           = svn:mime-type=application/octet-stream
*.nitf                          = svn:mime-type=application/octet-stream
*.ntf                           = svn:mime-type=application/octet-stream
*.mat                           = svm:mime-type=application/octet-stream
*.bmp                           = svm:mime-type=image/bmp
*.jpg                           = svm:mime-type=image/jpg
*.png                           = svm:mime-type=image/png
*.tif                           = svm:mime-type=image/tif
*.gif                           = svm:mime-type=image/gif



# Windows files
*.bat                           = svn:mime-type=application/x-msdos-program;svn:eol-style=CRLF;svn:executable
*.exe                           = svn:mime-type=application/x-msdos-program;svn:eol-style=CRLF;svn:executable
*.rc                            = svn:mime-type=text/plain;svn:eol-style=CRLF
*.doc                           = svn:mime-type=application/msword
*.ppt                           = svn:mime-type=application/msword

# Text files
*.txt*                          = svn:mime-type=text/plain
*.cmake                         = svn:mime-type=text/plain
*.cmake.in                      = svn:mime-type=text/plain
*.log                           = svn:mime-type=text/plain
CHANGELOG                       = svn:mime-type=text/plain
COPYING*                         = svn:mime-type=text/plain
COPYRIGHT                       = svn:mime-type=text/plain
LICENSE                         = svn:mime-type=text/plain
README*                         = svn:mime-type=text/plain
TODO                            = svn:mime-type=text/plain
AUTHORS                         = svn:mime-type=text/plain


# Source files
*.C                             = svn:mime-type=text/x-csrc
*.c                             = svn:mime-type=text/x-csrc
*.cpp                           = svn:mime-type=text/x-c++src
*.cc                            = svn:mime-type=text/x-c++src
*.cxx                           = svn:mime-type=text/x-c++src
*.cxx.in                        = svn:mime-type=text/x-c++src
*.H                             = svn:mime-type=text/x-chdr
*.h                             = svn:mime-type=text/x-chdr
*.P                             = svn:mime-type=text/x-chdr
*.hh                            = svn:mime-type=text/x-c++hdr
*.hpp                           = svn:mime-type=text/x-c++hdr
*.h.in                          = svn:mime-type=text/x-chdr
*.txx                           = svn:mime-type=text/x-c++src
*.tpp                           = svn:mime-type=text/x-c++src
*.m                             = svn:mime-type=text/x-matlab-src
*.htm                           = svn:mime-type=text/htm
*.html                          = svn:mime-type=text/html
*.css                           = svn:mime-type=text/css

# Build files
Makefile                        = svn:mime-type=text/x-makefile

# Script files
*.pl                            = svn:mime-type=text/x-perl;svn:executable
*.perl                          = svn:mime-type=text/x-perl;svn:executable
*.py                            = svn:mime-type=text/x-python
*.sh                            = svn:mime-type=text/x-sh;svn:eol-style=LF;svn:executable

# Text-format documents
*.xml                           = svn:mime-type=application/xml;svn:eol-style=native
*.pdf                           = svn:mime-type=application/pdf
*.ps                            = svn:mime-type=application/postscript
*.tex                           = svn:mime-type=application/tex

# Other files
*.RPB                           = svn:mime-type=text/plain
*.rpb                           = svn:mime-type=text/plain
*.ply                           = svn:mime-type=text/plain
*.xrc                           = svn:mime-type=application/xml;svn:eol-style=native
*.dat                           = svn:mime-type=text/plain
*.wrl                           = svn:mime-type=x-world/x-vrml;svn:eol-style=native
*.svg                           = svn:mime-type=image/svg+xml
*.tgz                           = svn:mime-type=application/x-compressed
*.gz                            = svn:mime-type=application/x-compressed
*.zip                           = svn:mime-type=application/x-compressed
*.vox                           = svm:mime-type=application/octet-stream
*.fig                           = svn:mime-type=application/x-fig
*.zuml                          = svn:mime-type=application/x-uml

